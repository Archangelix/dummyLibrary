# Books schema

# --- !Ups

CREATE SEQUENCE book_id_seq;

CREATE TABLE BOOK (
	id integer not null default nextval('book_id_seq'),
	title varchar2(100),
	author varchar2(50),
	publishedYear integer
);

INSERT INTO BOOK (ID, TITLE, AUTHOR, PUBLISHEDYEAR) VALUES (BOOK_ID_SEQ, 'ONE', 'SONE', 1901);
INSERT INTO BOOK (ID, TITLE, AUTHOR, PUBLISHEDYEAR) VALUES (BOOK_ID_SEQ, 'TWO', 'STWO', 1902);
INSERT INTO BOOK (ID, TITLE, AUTHOR, PUBLISHEDYEAR) VALUES (BOOK_ID_SEQ, 'THREE', 'STHREE', 1903);

# --- !Downs
DROP TABLE BOOK;
DROP SEQUENCE book_id_seq;
